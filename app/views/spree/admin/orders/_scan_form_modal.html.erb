<div class="modal fade scan-modal" id="formModal" tabindex="-1" role="dialog" aria-labelledby="formModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header scan-modal__header">
        <h5 class="modal-title text-center" id="formModalLabel"><%= Spree.t(:scan_form) %></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <%= select_tag 'stockLocation', options_from_collection_for_select(Spree::StockLocation.all, "id", "admin_name"), { class: 'form-control' } %>
        <%= hidden_field :api_key, :token, value: spree_current_user.spree_api_key %>


      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
        <button type="button" id="submitScanForm" class="btn btn-primary">Save changes</button>
      </div>
    </div>
  </div>
</div>