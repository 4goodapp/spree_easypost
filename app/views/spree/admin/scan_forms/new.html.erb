<% content_for :page_title do %>
  <%= link_to Spree.t(:scan_forms), spree.admin_scan_forms_url %> /
  <%= Spree.t(:new_scan_form) %>
<% end %>

<%= render :partial => 'spree/admin/shared/error_messages', :locals => { :target => @scan_form } %>

<%= form_for [:admin, @scan_form] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { :f => f } %>
    <%= render :partial => 'spree/admin/shared/new_resource_links' %>
  </fieldset>
<% end %>
